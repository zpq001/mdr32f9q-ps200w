S	xTimeNow = xTaskGetTickCount();
S
S	if( xTimeNow < xLastTime )
S	{
S		prvSwitchTimerLists( xLastTime );
S		*pxTimerListsWereSwitched = pdTRUE;
S	}
S	else
S	{
S		*pxTimerListsWereSwitched = pdFALSE;
S	}
S
S	xLastTime = xTimeNow;
S
S	return xTimeNow;
S}
